# Porta do servidor
PORT=3000

# Segredo JWT (altere em produção)
JWT_SECRET=troque_este_segredo

# ID da planilha (Spreadsheet ID)
# Exemplo: 1AbCdEfGhIjKlMnOpQrStUvWxYz1234567890
GOOGLE_SHEETS_SPREADSHEET_ID=

# Título da aba com os contatos
GOOGLE_SHEETS_SHEET_TITLE=Contacts

# Caminho do arquivo de credencial da Service Account (JSON)
GOOGLE_SERVICE_ACCOUNT_KEY_FILE=./credentials/service-account.json

# Permitir cadastro público de usuários (true/false)
ALLOW_PUBLIC_REGISTRATION=true